<% include ./partials/header %>

<h1>Here</h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src = "<%= blog.image %>"></img>
        <span><%= blog.created %></span>
        <p><%= blog.body.substring(0, 100)%>...</p>
        <a href="/blogs/<%= blog._id %>">Show More</a>
    </div>
<% }) %>

<% include ./partials/footer %>
